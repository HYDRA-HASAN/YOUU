{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/public/$1"
    }
  ],
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node@18"
    }
  }
}